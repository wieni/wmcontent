{
    "name": "wieni/wmcontent",
    "type": "drupal-module",
    "description": "Adds configurable entity containers to entity types (for e.g. paragraphs)",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "drupal/core": "^8.7.7 || ^9"
    },
    "require-dev": {
        "drupal/core-dev": "^8.5 || ^9",
        "ergebnis/composer-normalize": "^2.0",
        "wieni/wmcodestyle": "^1.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php",
            "phpstan analyse"
        ]
    }
}
